{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["DOM", "ES2020"],
    "rootDir": ".",
    "outDir": "dist",
    "jsx": "react-jsx",
    "useDefineForClassFields": true,
    "moduleResolution": "bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": ["backend/src"]
}
